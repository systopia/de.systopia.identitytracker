<?xml version="1.0"?>
<extension key="de.systopia.identitytracker" type="module">
  <file>identitytracker</file>
  <name>Contact Identity Tracker</name>
  <description>The sole purpose of this extension is to keep a history of contact ID and external identfier that would otherwise be destroyed when merging duplicates</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.identitytracker</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.identitytracker</url>
    <url desc="Support">https://github.com/systopia/de.systopia.identitytracker/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>1.5-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <comments>This extension has initially been funded by Amnesty International Vlaanderen</comments>
  <civix>
    <namespace>CRM/Identitytracker</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <upgrader>CRM_Identitytracker_Upgrader</upgrader>
</extension>
